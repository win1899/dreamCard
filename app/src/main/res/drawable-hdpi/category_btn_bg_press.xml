<?xml version="1.0" encoding="utf-8"?>


<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape>
            <solid
                android:color="#DBEFA8" />
            <stroke
                android:width="1dp"
                android:color="#F1F1F3" />
            <corners
                android:radius="0dp" />
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#ffffff"
                android:endColor="#ffffff"
                android:angle="270" />
            <stroke
                android:width="2dp"
                android:color="#F1F1F3" />
            <corners
                android:radius="0dp" />
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp" />
        </shape>
    </item>
</selector>